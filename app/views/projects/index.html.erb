<h1>Projects</h1>

<td><%= link_to 'New', new_project_path %></td>

<table class="table">
  <tr>
    <th scope="col">Name</th>
    <th scope="col"></th>
  </tr>

  <% @projects.each do |project| %>
    <tr>
      <td scope="row"><%= project.name %></td>
      <td scope="row"><%= link_to 'Show', project_path(project) %></td>
    </tr>
  <% end %>
</table>
